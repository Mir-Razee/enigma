        document.getElementById("M").style.boxShadow = "0px 0px 0px black";
        document.getElementById("A").style.boxShadow = "0px 0px 0px black";
        document.getElementById("ENTER").style.boxShadow = "0px 0px 0px black";
        counter = 0;
      }
      elem.innerHTML = text[counter];
      if (text2[counter] != "") {
        document.getElementById(text2[counter]).style.boxShadow = "0px 0px 10px red";
      }
      counter++;
    }

    var text = ["", "Q", "QZ", "QZY", "QZYC", "QZYCN", "QZYCNO", "QZYCNO "];
    var text2 = ["", "E", "N", "I", "G", "M", "A", "ENTER"];
    var counter = 0;
    var elem = document.getElementById("output");
    elem.innerHTML = "WELCOME"
    var inst = setInterval(change, 1000);

    function change() {
      if (counter >= text.length) {
        document.getElementById("E").style.boxShadow = "0px 0px 0px black";
        document.getElementById("N").style.boxShadow = "0px 0px 0px black";
        document.getElementById("I").style.boxShadow = "0px 0px 0px black";
